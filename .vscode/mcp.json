{
  "servers": {
    "todo-mcp-server": {
      "type": "stdio",
      "command": "node",
      "args": ["build/server.js"],
      "cwd": "${workspaceFolder}",
      "dev": {
        "watch": "build/**/*.ts",
        "debug": {
          "type": "node"
        }
      }
    },
    "todo-mcp-http-streamable-server": {
      "url": "http://localhost:4000/mcp",
      "type": "http"
    }
  },
  "inputs": []
}
